{
  "schemaVersion": 1,

  "id": "splatcraft",
  "version": "${version}",

  "name": "Splatcraft",
  "icon": "assets/splatcraft/icon.png",
  "description": "A Minecraft mod based on Nintendoâ€™s popular game Splatoon. This mod aims to add items and mechanics based on the game in a cohesive way, such as weapons, squid transformation, Turf War and other features!",

  "license": [
    "MIT (Code)",
    "All Rights Reserved (Assets)"
  ],
  "authors": [
    "Daniel Orr (Andante)",
    "Gaetano Russo (Ninni)"
  ],
  "contributors": [ "",
    "Cibernet (Former Author)"
  ],

  "contact": {
    "homepage": "https://splatcraft.net",
    "sources": "https://github.com/splatcraft/splatcraft-fabric",
    "issues": "https://github.com/splatcraft/splatcraft-fabric/issues"
  },

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.splatcraft.impl.SplatcraftImpl"
    ],
    "client": [
      "net.splatcraft.impl.client.SplatcraftClientImpl"
    ],

    "frame-registries:end_dynamic_registry_setup": [ "net.splatcraft.impl.registry.SplatcraftRegistryImpl" ],

    "modmenu": [ "net.splatcraft.impl.client.integration.SplatcraftModMenuImpl" ],

    "fabric-datagen": [ "net.splatcraft.impl.data.SplatcraftDataGeneratorImpl" ]
  },

  "mixins": [ "splatcraft.mixins.json" ],
  "accessWidener": "splatcraft.accesswidener",

  "custom": {
    "cardinal-components": [
    ],
    "modmenu": { "links": { "mod.splatcraft.contact.discord": "https://discord.splatcraft.net" } }
  },

  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19",
    "fabricloader": ">=0.14",
    "fabric": ">=0.57",
    "frame": ">=0.26",
    "cardinal-components": ">=5.0"
  },
  "suggests": {
    "modmenu": "*",
    "sodium": "*",
    "trinkets": "*"
  }
}
